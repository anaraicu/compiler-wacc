# for loop with break and continue inside if statements

# Output: 
# 0
# 1 
# 6
# 3

# Program:

begin
  int x = 0;
  for (int a = 0; a < 2; a=a+1)
    println a;
    x = x + 1;
    if (a == 1) then
        int y = x + 3;
        println y;
        break
    else
        x = x + 1;
        continue
    fi;
    x = x + 1000
  done ;
  println x
end
